{
    "name": "jmerilainen/satis-builder",
    "description": "This is my package satis-builder",
    "keywords": [
        "jmerilainen",
        "satis-builder"
    ],
    "homepage": "https://github.com/jmerilainen/satis-builder",
    "license": "MIT",
    "authors": [
        {
            "name": "Joonas Meril√§inen",
            "email": "merilainenjoonas@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.0",
        "symfony/console": "^6.0",
        "symfony/finder": "^6.0"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.0",
        "pestphp/pest": "^1.20",
        "spatie/ray": "^1.28"
    },
    "autoload": {
        "psr-4": {
            "Jmerilainen\\SatisBuilder\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Jmerilainen\\SatisBuilder\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/pest",
        "test-coverage": "vendor/bin/pest --coverage",
        "format": "vendor/bin/php-cs-fixer fix --allow-risky=yes"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "bin": [
        "bin/satis-builder"
    ]
}
